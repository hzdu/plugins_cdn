{
    "name": "checkout-for-woocommerce",
    "version": "10.0.3",
    "description": "### Development installation instructions",
    "scripts": {
        "build_dev": "./bin/build-dev.sh",
        "build_prod": "./bin/build-prod.sh",
        "dev": "npx webpack --config webpack.dev.js",
        "production-travis": "cross-env CFW_TRAVIS=1 webpack --config webpack.prod.js",
        "watch": "npm run dev -- --watch",
        "composer_install": "composer install --no-dev --ignore-platform-reqs && composer dump-autoload",
        "composer_install_dev": "composer install --ignore-platform-reqs && composer dump-autoload",
        "min_php_version_test": "./vendor/bin/phpcs -p . --standard=PHPCompatibility --runtime-set testVersion 7.3 -n --extensions=php,inc,lib --ignore=node_modules/*,dist/*,vendor/*,tests/*,vendor-prefixed/sabberworm/php-css-parser/src/RuleSet/DeclarationBlock.php",
        "max_php_version_test": "./vendor/bin/phpcs -p . --standard=PHPCompatibility --runtime-set testVersion 8.3 -n --extensions=php,inc,lib --ignore=node_modules/*,dist/*,vendor/*,tests/*,vendor-prefixed/sabberworm/php-css-parser/src/RuleSet/DeclarationBlock.php",
        "phpunit": "./vendor/bin/phpunit -c phpunit.xml --testdox",
        "tailwind_admin": "npx tailwind --output './sources/scss/admin/tailwind.css' --watch --config=tailwind.admin.config.js",
        "tailwind_frontend": "npx tailwind --output './sources/scss/frontend/tailwind.css' --watch --config=tailwind.frontend.config.js",
        "build": "wp-scripts build",
        "start": "cross-env NODE_ENV=development wp-scripts start",
        "start_and_analyze": "cross-env NODE_ENV=development WP_BUNDLE_ANALYZER=true wp-scripts start",
        "build_and_zip": "cross-env NODE_ENV=production wp-scripts build --config webpack.prod.js",
        "preinstall": "npx only-allow pnpm",
        "generate-documentation": "./bin/generate-documentation.sh",
        "generate-action-documentation": "./vendor/bin/wp-documentor parse . --ignore-vcs-ignored --prefix=cfw --prefix=checkoutwc --type=actions --exclude=vendor --exclude=vendor-prefixed --exclude=dist --exclude=node_modules --exclude=build --format=markdown --output=docs/actions.md",
        "generate-filter-documentation": "./vendor/bin/wp-documentor parse . --ignore-vcs-ignored --prefix=cfw --prefix=checkoutwc --type=filters --exclude=vendor --exclude=vendor-prefixed --exclude=dist --exclude=node_modules --exclude=build --format=markdown --output=docs/filters.md",
        "test:build": "wp-env start",
        "test:db:export": "./tests/e2e/setup/dbexport.sh",
        "test:db:import": "./tests/e2e/setup/dbimport.sh",
        "test:rebuild": "echo 'Y' | wp-env destroy && wp-env start",
        "test:headless": "pnpm playwright test",
        "test": "pnpm playwright test --ui"
    },
    "browserslist": [
        "> 1%",
        "last 1 Android versions",
        "last 1 ChromeAndroid versions",
        "last 2 Chrome versions",
        "last 2 Firefox versions",
        "last 2 Safari versions",
        "last 2 iOS versions",
        "last 2 Edge versions",
        "last 2 Opera versions"
    ],

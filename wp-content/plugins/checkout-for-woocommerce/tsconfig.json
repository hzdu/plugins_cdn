{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2015",
        "sourceMap": true,
        "allowJs": true,
        "jsx": "react",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "composite": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "ES2015",
        ],
        "moduleResolution": "node",
    },
    "include": [
        "./sources/ts/**/*"
    ],
    "files": [
        "node_modules/@woocommerce/components/build/search/autocompleters/utils.js",
    ],
    "exclude": [
        "./sources/ts/tests/**/*"
    ]
}
{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "dracula/switch",
  "version": "1.0.0",
  "title": "Dark Mode Switch",
  "category": "widgets",
  "description": "Insert a dark mode toggle switch into your content.",
